<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="swith.studygroupmembermapper">

	<resultMap type="StudyGroupMemberDto" id="studyGroupMemberMap">
		<result property="study_group_member" column="STUDY_GROUP_MEMBER" />
		<result property="study_group_id" column="STUDY_GROUP_ID" />
		<result property="member_email" column="MEMBER_EAMIL" />
		<result property="role" column="ROLE" />
	</resultMap>

	<select id="StudyMemberCount" parameterType="int">
		SELECT COUNT(*)
		FROM STUDY_GROUP_MEMBER
		WHERE STUDY_GROUP_ID = #{study_group_id}
	</select>
</mapper>

